  "description": "enable limited interaction with a content editable element, useful for in-place, wysiwyg editing",
  "main": "index.js",
  "scripts": {
    "test": "karma start",
    "e2e": "node_modules/nightwatch/bin/nightwatch --test __tests__/e2e/*"
